<script>
    var x = window.matchMedia("(max-width: 700px)");

    function reverseElements(x) {
        var section = document.getElementById('#psr').childNodes;

        var sectionParentElement = section[1];
        console.log(sectionParentElement)
        var element = sectionParentElement.childNodes;

        if (x.matches) { // If media query matches
            console.log(element);
            element[1].classList.add("flex-column-reverse");
            console.log(element);

        } else {
            console.log(element);

            element[1].classList.remove("flex-column-reverse");
        }
    }

    reverseElements(x);
</script>


