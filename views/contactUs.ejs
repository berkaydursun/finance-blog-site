<%- include('./partials/header.ejs') %>





    <section id="sendMessage">

        <div class="container">
            <h3 class="color-primary text-center ">Contact Us</h3>
            <form class="mt-50" method="POST">
                <div class="row mt-50">
                    <div class="col">
                        <input type="text" class="form-control" placeholder="First name" name="firstName">
                    </div>
                    <div class="col">
                        <input type="text" class="form-control" placeholder="Last name" name="lastName">
                    </div>
                </div>
                <div class="form-group row mt-50">

                    <div class="col-sm-12">
                        <input type="email" class="form-control" id="inputEmail3" placeholder="Email" name="email">
                    </div>
                </div>
                <div class="form-group mt-50">
                    <textarea class="form-control" rows="14" placeholder="Type Message ..." name="text"></textarea>
                </div>
                <button type="submit" class="btn btn-dark btn-block mt-50">Submit</button>
            </form>
        </div>

    </section>






    <%- include('./partials/footer.ejs') %>