    <section id="sendMessage">

        <div class="container">
            <h3 class="color-primary text-center ">Contact Us</h3>
            <form class="mt-50">
                <div class="row mt-50">
                    <div class="col">
                        <input type="text" class="form-control" placeholder="First name">
                    </div>
                    <div class="col">
                        <input type="text" class="form-control" placeholder="Last name">
                    </div>
                </div>
                <div class="form-group row mt-50">

                    <div class="col-sm-12">
                        <input type="email" class="form-control" id="inputEmail3" placeholder="Email">
                    </div>
                </div>
                <div class="form-group mt-50">
                    <textarea class="form-control" id="FormControlTextarea1" rows="3" placeholder="Type Message ..."></textarea>
                </div>
                <button type="submit" class="btn btn-dark btn-block mt-50">Submit</button>
            </form>
        </div>

    </section>






